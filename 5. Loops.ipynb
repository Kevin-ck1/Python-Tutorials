   "source": [
    "#Quiz, Compute all multiples of 3, 5 that are less than 100\n",
    "# range(1,100) "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2318\n"
     ]
    }
   ],
   "source": [
    "#Solution\n",
    "\n",
    "t = 0\n",
    "\n",
    "for i in range(1,100):\n",
    "    if i % 3 == 0 or i % 5 == 0 :\n",
    "        t += i\n",
    "        \n",
    "print(t)"
   ]
